@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .

map:database a d2rq:Database;
	d2rq:jdbcDriver "com.mysql.jdbc.Driver";
	d2rq:jdbcDSN "jdbc:mysql:///nlp_homework?useSSL=false";
	d2rq:username "root";
	d2rq:password "daixinle.1205";
	jdbc:autoReconnect "true";
	jdbc:zeroDateTimeBehavior "convertToNull";
	.

# Table address
map:address a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "address/@@address.id@@";
	d2rq:class vocab:address;
	d2rq:classDefinitionLabel "address";
	.
map:address__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:address;
	d2rq:property rdfs:label;
	d2rq:pattern "address #@@address.id@@";
	.
map:address_id a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:address;
	d2rq:property vocab:address_id;
	d2rq:propertyDefinitionLabel "address id";
	d2rq:column "address.id";
	d2rq:datatype xsd:integer;
	.
map:address_city a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:address;
	d2rq:property vocab:address_city;
	d2rq:propertyDefinitionLabel "address city";
	d2rq:column "address.city";
	.

# Table personinfo
map:personinfo a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "personinfo/@@personinfo.id@@";
	d2rq:class vocab:personinfo;
	d2rq:classDefinitionLabel "personinfo";
	.
map:personinfo__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:personinfo;
	d2rq:property rdfs:label;
	d2rq:pattern "personinfo #@@personinfo.id@@";
	.
map:personinfo_id a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:personinfo;
	d2rq:property vocab:personinfo_id;
	d2rq:propertyDefinitionLabel "personinfo id";
	d2rq:column "personinfo.id";
	d2rq:datatype xsd:integer;
	.
map:personinfo_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:personinfo;
	d2rq:property vocab:personinfo_name;
	d2rq:propertyDefinitionLabel "personinfo name";
	d2rq:column "personinfo.name";
	.
map:personinfo_addr__ref a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:personinfo;
	d2rq:property vocab:personinfo_addr;
	d2rq:refersToClassMap map:address;
	d2rq:join "personinfo.addr => address.id";
	.

